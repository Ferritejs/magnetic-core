{
  "diff": true,
  "extension": ["js", "cjs", "mjs", "ts"],
  "type": "module",
  "package": "./package.json",
  "reporter": "spec",
  "slow": "75",
  "timeout": "2000",
  "ui": "bdd",
  "require": ["ts-node/register"],
  "spec": ["test/**/*.spec.ts", "test/**/*.spec.ts"],
  "watch-files": ["src/**/*.js", "src/**/*.ts", "test/**/*.js", "test/**/*.ts"]
  // "watch-ignore": ["lib/vendor"]
}
